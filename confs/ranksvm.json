{
  "algorithm" : "RankSVM",
  "numIterations" : 100,
  "batchSize" : 10,
  "verbose": true,
  "params" : {
    "learningRate" : 0.01,
    "optimizer" : "sgd",
    "weightInit" : "gaussian",
    "kernel" : "linear",
    "optMetric" : true
  },

  "dataSet" : {
    "training" : "data/MQ2008/Fold1/train.txt",
    "validation" : "data/MQ2008/Fold1/vali.txt",
    "test" : "data/MQ2008/Fold1/test.txt"
  },

  "model" : {
    "format" : "json",
    "file" : "model/ranknet-model.json"
  },

  "evaluation" : {
    "evaluator" : "ndcg",
    "params" : {
      "k" : 10
    }
  },

  "report" : {
    "format" : "csv",
    "file" : "report/ranknet-report.csv"
  }
}